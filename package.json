{
  "name": "nest-mongo-boilerplate",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "license": "MIT",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "ts-node -r tsconfig-paths/register src/backend/main.ts",
    "start:dev": "concurrently --handle-input \"wait-on dist/backend/main.js && nodemon\" \"tsc -w -p tsconfig.build.json\" ",
    "start:debug": "nodemon --config nodemon-debug.json",
    "prestart:prod": "rimraf dist && npm run build",
    "start:prod": "node dist/backend/main.js",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx ./src ./test",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json --forceExit"
  },
  "dependencies": {
    "@nestjs/common": "^6.10.14",
    "@nestjs/core": "^6.10.14",
    "@nestjs/platform-express": "^6.10.14",
    "@nestjs/swagger": "^4.1.12",
    "@typegoose/typegoose": "^6.2.1",
    "nestjs-typegoose": "^7.0.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-router-dom": "^5.1.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.0",
    "rxjs": "^6.5.4",
    "swagger-ui-express": "^4.1.2",
    "@nestjs/mongoose": "^6.2.1",
    "mongoose": "^5.8.7",
    "parcel-bundler": "^1.12.4"
  },
  "devDependencies": {
    "@nestjs/testing": "^6.10.14",
    "@types/express": "^4.17.2",
    "@types/jest": "^24.9.0",
    "@types/node": "^13.1.8",
    "@types/supertest": "^2.0.8",
    "@types/mongoose": "^5.5.41",
    "concurrently": "^5.0.2",
    "jest": "^25.0.0",
    "nodemon": "^2.0.2",
    "prettier": "^1.19.1",
    "supertest": "^5.0.0-0",
    "ts-jest": "24.3.0",
    "ts-node": "8.6.2",
    "tsconfig-paths": "3.9.0",
    "typescript": "3.7.5",
    "wait-on": "^3.3.0",
    "@typescript-eslint/eslint-plugin": "^2.16.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-typescript": "^6.3.1",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-jest": "^23.6.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prefer-arrow": "^1.1.7",
    "eslint-plugin-react": "^7.18.0",
    "eslint-plugin-jsdoc": "^20.3.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
